"_note": "to apply snippet require reloading atom or press ctrl-alt-r"

".source":
  "BUG":
    prefix: "bug"
    body: "BUG"
  "NOTE":
    prefix: "note"
    body: "NOTE"
  "TODO":
    prefix: "todo"
    body: "TODO"

".source.js":
  "(() => {...})()":
    prefix: "iife"
    body: """
      (() => {

      })()"""
  "map(fcn(...))":
    prefix: "mapf"
    body: """
      map(function($1){
        $2
        return ;
      });"""

  "sort(fcn(...))":
    prefix: "sortf"
    body: """
      sort(function(${1:a}, ${2:b}){return $3;});"""

  "it('spec', ()=>{})":
    prefix: "it"
    body: """
      it('${1:spec description}', (done) => {
        done();
        $2
      });"""

  "import... export...":
    prefix: "init"
    body: """
      import React from 'react'

      const ${1:myComponent} = () => {
        return (
          <${2:div}></${2:div}>
        )
      }

      export default ${1:myComponent}"""

".source.coffee":
  "it('msg', ()->{})":
    prefix: "it"
    body: """
      it "${1:spec description}", ->
        ${2:foo()}"""

".source.java":
  "System.out.println()":
    prefix: "sout"
    body: "System.out.println(${1:\"\"});$2"

".source.php":
  ".'<br>'":
    prefix: "br"
    body: ".'<br>'"

".source.python":
  "__init__(self)":
    prefix: "init"
    body: """
      def __init__(self, ${1:arg}):
          self.${1:arg} ="""

  "if __name__ == '__main__'":
    prefix: "ifmain"
    body: """
      if __name__ == '__main__':
          ${1:foo()}"""

